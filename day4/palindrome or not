#include <stdio.h>

int main()
{
   int n,rem,reverse=0,copy;
   int palindrome=1,product=0,sum=0;
   scanf("%d",&n);
   copy =n;
   while(n!=0)
   {
       rem = n%10;
       reverse = reverse * 10+ rem;
       n /=10;
   }
   if(reverse==copy)
   {
       printf("palindrome.");
   }
   else
   
   {
       printf("not palindrome.");
   }
}
