#include <stdio.h>

int main()
{
    int arr[100];
    int n,i,j,mx;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        
    }
    mx=arr[0];
    for(i=1;i<n;i++)
    {
        if(arr[i]>mx)
        {
            mx=arr[i];
        }
    }
    printf("the peak number:%d",mx);
    return 0;
}
